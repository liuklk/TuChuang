<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@color/stock_bg"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="350dp">
    <com.baidu.mapapi.map.TextureMapView
        android:id="@+id/mTexturemap"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    </com.baidu.mapapi.map.TextureMapView>
<ImageView
    android:id="@+id/logistics_back"
    android:padding="7dp"
    android:background="@drawable/map_back"
    android:src="@drawable/nav_back"
    android:layout_marginLeft="12dp"
    android:layout_marginTop="37dp"
    android:layout_width="30dp"
    android:layout_height="30dp" />
    <ImageView
        android:id="@+id/call_phone"
        android:layout_alignParentRight="true"
        android:padding="7dp"
        android:background="@drawable/map_back"
        android:src="@drawable/map_phone"
        android:layout_marginRight="12dp"
        android:layout_marginTop="37dp"
        android:layout_width="30dp"
        android:layout_height="30dp" />
</RelativeLayout>
    <LinearLayout
        android:layout_marginTop="16dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:gravity="center"
            android:layout_weight="1"
            android:textColor="@color/text_bg_3"
            android:textSize="@dimen/text_size_14"
            android:text="已提交"
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
        <TextView
            android:gravity="center"
            android:layout_weight="1"
            android:textColor="@color/text_bg_1"
            android:textSize="@dimen/text_size_16"
            android:text="待配送"
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
        <TextView
            android:gravity="center"
            android:layout_weight="1"
            android:textColor="@color/text_bg_3"
            android:textSize="@dimen/text_size_14"
            android:text="配送中"
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
        <TextView
            android:gravity="center"
            android:layout_weight="1"
            android:textColor="@color/text_bg_3"
            android:textSize="@dimen/text_size_14"
            android:text="等待送达"
            android:layout_width="0dp"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_marginTop="5dp"
        android:layout_width="match_parent"
        android:layout_height="5dp">
        <LinearLayout
            android:layout_weight="1"
            android:orientation="horizontal"
            android:layout_width="0dp"
            android:layout_height="match_parent">
            <View
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"/>
            <View
                android:background="@drawable/logistics_normal"
                android:layout_width="5dp"
                android:layout_height="5dp"/>
            <ImageView
                android:layout_gravity="center_vertical"
                android:background="@drawable/dingdanxiangqin_line"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            android:layout_weight="1"
            android:orientation="horizontal"
            android:layout_width="0dp"
            android:layout_height="match_parent">
            <ImageView
                android:layout_gravity="center_vertical"
                android:background="@drawable/dingdanxiangqin_line"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"/>
            <View
                android:background="@drawable/logistics_select"
                android:layout_width="5dp"
                android:layout_height="5dp"/>
            <ImageView
                android:layout_gravity="center_vertical"
                android:background="@drawable/dingdanxiangqin_line"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            android:layout_weight="1"
            android:orientation="horizontal"
            android:layout_width="0dp"
            android:layout_height="match_parent">
            <ImageView
                android:layout_gravity="center_vertical"
                android:background="@drawable/dingdanxiangqin_line"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"/>
            <View
                android:background="@drawable/logistics_normal"
                android:layout_width="5dp"
                android:layout_height="5dp"/>
            <ImageView
                android:layout_gravity="center_vertical"
                android:background="@drawable/dingdanxiangqin_line"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            android:layout_weight="1"
            android:orientation="horizontal"
            android:layout_width="0dp"
            android:layout_height="match_parent">
            <ImageView
                android:layout_gravity="center_vertical"
                android:background="@drawable/dingdanxiangqin_line"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"/>
            <View
                android:background="@drawable/logistics_normal"
                android:layout_width="5dp"
                android:layout_height="5dp"/>
            <View
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"/>
        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_weight="1"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="0dp">
        <RelativeLayout
            android:layout_weight="1"
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="0dp">
            <TextView
                android:textColor="@color/text_bg_2"
                android:textSize="@dimen/text_size_14"
                android:id="@+id/yitijiao"
                android:layout_marginLeft="42dp"
                android:drawablePadding="10dp"
                android:drawableLeft="@drawable/logistics_normal_big"
                android:text="订单已提交"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:textColor="@color/text_bg_3"
                android:layout_alignBaseline="@id/yitijiao"
                android:textSize="@dimen/text_size_10_"
                android:layout_toRightOf="@id/yitijiao"
                android:layout_marginLeft="10dp"
                android:text="2017-11-03 17: 23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <View
                android:layout_below="@id/yitijiao"
                android:layout_marginLeft="45dp"
                android:background="@color/strock"
                android:layout_width="1dp"
                android:layout_height="24dp"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp">
            <TextView
                android:textColor="@color/text_bg_2"
                android:textSize="@dimen/text_size_14"
                android:id="@+id/yifahuo"
                android:layout_marginLeft="42dp"
                android:drawablePadding="10dp"
                android:drawableLeft="@drawable/logistics_normal_big"
                android:text="已发货"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:textColor="@color/text_bg_3"
                android:layout_alignBaseline="@id/yifahuo"
                android:textSize="@dimen/text_size_10_"
                android:layout_toRightOf="@id/yifahuo"
                android:layout_marginLeft="10dp"
                android:text="2017-11-03 17: 23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <View
                android:layout_below="@id/yifahuo"
                android:layout_marginLeft="45dp"
                android:background="@color/strock"
                android:layout_width="1dp"
                android:layout_height="24dp"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp">
            <TextView
                android:textColor="@color/text_bg_2"
                android:textSize="@dimen/text_size_14"
                android:id="@+id/daipeisong"
                android:layout_marginLeft="42dp"
                android:drawablePadding="10dp"
                android:drawableLeft="@drawable/logistics_select_big"
                android:text="订单待配送"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:textColor="@color/text_bg_3"
                android:layout_alignBaseline="@id/daipeisong"
                android:textSize="@dimen/text_size_10_"
                android:layout_toRightOf="@id/daipeisong"
                android:layout_marginLeft="10dp"
                android:text="2017-11-03 17: 23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <View
                android:layout_below="@id/daipeisong"
                android:layout_marginLeft="45dp"
                android:background="@color/strock"
                android:layout_width="1dp"
                android:layout_height="24dp"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp">
            <TextView
                android:textColor="@color/text_bg_2"
                android:textSize="@dimen/text_size_14"
                android:id="@+id/peisongzhong"
                android:layout_marginLeft="42dp"
                android:drawablePadding="10dp"
                android:drawableLeft="@drawable/logistics_normal_big"
                android:text="订单配送中"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:textColor="@color/text_bg_3"
                android:layout_alignBaseline="@id/peisongzhong"
                android:textSize="@dimen/text_size_10_"
                android:layout_toRightOf="@id/peisongzhong"
                android:layout_marginLeft="10dp"
                android:text="2017-11-03 17: 23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <View
                android:layout_below="@id/peisongzhong"
                android:layout_marginLeft="45dp"
                android:background="@color/strock"
                android:layout_width="1dp"
                android:layout_height="24dp"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp">
            <TextView
                android:textColor="@color/text_bg_2"
                android:textSize="@dimen/text_size_14"
                android:id="@+id/dengdai"
                android:layout_marginLeft="42dp"
                android:drawablePadding="10dp"
                android:drawableLeft="@drawable/logistics_normal_big"
                android:text="等待送达"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:textColor="@color/text_bg_3"
                android:layout_alignBaseline="@id/dengdai"
                android:textSize="@dimen/text_size_10_"
                android:layout_toRightOf="@id/dengdai"
                android:layout_marginLeft="10dp"
                android:text="2017-11-03 17: 23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <View
                android:layout_below="@id/dengdai"
                android:layout_marginLeft="45dp"
                android:background="@color/strock"
                android:layout_width="1dp"
                android:layout_height="24dp"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp">
            <TextView
                android:textColor="@color/text_bg_2"
                android:textSize="@dimen/text_size_14"
                android:id="@+id/shouhuo"
                android:layout_marginLeft="42dp"
                android:drawablePadding="10dp"
                android:drawableLeft="@drawable/logistics_normal_big"
                android:text="确认收货"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:textColor="@color/text_bg_3"
                android:layout_alignBaseline="@id/shouhuo"
                android:textSize="@dimen/text_size_10_"
                android:layout_toRightOf="@id/shouhuo"
                android:layout_marginLeft="10dp"
                android:text="2017-11-03 17: 23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <View
                android:layout_below="@id/shouhuo"
                android:layout_marginLeft="45dp"
                android:background="@color/strock"
                android:layout_width="1dp"
                android:layout_height="24dp"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp">
            <TextView
                android:textColor="@color/text_bg_2"
                android:textSize="@dimen/text_size_14"
                android:id="@+id/wancheng"
                android:layout_marginLeft="42dp"
                android:drawablePadding="10dp"
                android:drawableLeft="@drawable/logistics_normal_big"
                android:text="订单完成"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:textColor="@color/text_bg_3"
                android:layout_alignBaseline="@id/wancheng"
                android:textSize="@dimen/text_size_10_"
                android:layout_toRightOf="@id/wancheng"
                android:layout_marginLeft="10dp"
                android:text="2017-11-03 17: 23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </RelativeLayout>
    </LinearLayout>

</LinearLayout>